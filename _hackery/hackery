ifndef __mh_hackery_include_guard
override __mh_hackery_include_guard:=1


override empty:=
override tab:=$(empty)	$(empty)
override space:=$(empty) $(empty)
override comma:= ,
override semi:=;
override bspace:=$(empty)$(empty)
override ,:=$(comma)
override ;:=$(semi)


endif


